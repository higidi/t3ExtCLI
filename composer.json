{
    "name": "higidi/t3extcli",
    "description": "TYPO3 Extension Manager CLI Utility",
    "keywords": [
        "typo3"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel HÃ¼rtgen",
            "email": "daniel@higidi.de",
            "homepage": "http://www.higidi.de"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "typo3/cms",
                "version": "6.2.5",
                "source": {
                    "url": "http://git.typo3.org/Packages/TYPO3.CMS.git",
                    "type": "git",
                    "reference": "6.2.5"
                },
                "autoload": {
                    "classmap": [
                        "typo3/"
                    ]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "silex/silex": "~1.2",
        "cilex/cilex": "~1.1",
        "typo3/cms": "6.2.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "T3ExtCli": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "T3ExtCli\\Test": "tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "satooshi/php-coveralls": "dev-master"
    },
    "bin": [
        "bin/T3ExtCli"
    ],
    "extra": {
        "branch-alias": {
            "dev-development": "1.0.x-dev"
        }
    }
}
